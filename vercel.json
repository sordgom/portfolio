{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "rewrites": [
      {
        "source": "/static/(.*)",
        "destination": "/static/$1"
      },
      {
        "source": "/favicon.ico",
        "destination": "/favicon.ico"
      },
      {
        "source": "/manifest.json",
        "destination": "/manifest.json"
      },
      {
        "source": "/logo192.png",
        "destination": "/logo192.png"
      },
      {
        "source": "/logo512.png",
        "destination": "/logo512.png"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }